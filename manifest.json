{
    "name": "table-tools",
    "description": "this is tables tools",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background/index.js"
        ]
    },
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo32.png",
        "128": "images/logo48.png"
    },
    "browser_action": {
        "default_title": "plugin title",
        "default_popup": "./popup/index.html",
        "default_icon": "./images/logo.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens index.html"
        }
    }
}